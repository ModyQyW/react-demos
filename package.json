{
  "name": "react-demos",
  "version": "0.1.2",
  "private": true,
  "author": {
    "name": "ModyQyW",
    "email": "wurui7@mail3.sysu.edu.cn",
    "url": "https://modyqyw.github.io"
  },
  "scripts": {
    "build": "tsc && vite build --base=/react-demos/",
    "commit": "cz",
    "dev": "vite --host",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "preview": "vite preview",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@iconify/react": "~3.1.0",
    "@modyqyw/utils": "~2.0.1",
    "ahooks": "~3.0.7",
    "antd": "~4.17.4",
    "browser-update": "~3.3.35",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-router": "~6.2.1",
    "react-router-dom": "~6.2.1"
  },
  "devDependencies": {
    "@babel/core": "~7.16.5",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~15.0.0",
    "@modyqyw/fabric": "~4.4.2",
    "@types/browser-update": "~3.3.0",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.8.0",
    "@typescript-eslint/parser": "~5.8.0",
    "@vitejs/plugin-react": "~1.1.3",
    "commitizen": "~4.2.4",
    "eslint": "~8.5.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.3",
    "markdownlint-cli": "~0.30.0",
    "postcss-preset-env": "~7.0.2",
    "prettier": "~2.5.1",
    "release-it": "~14.11.8",
    "stylelint": "~14.1.0",
    "tailwindcss": "~3.0.7",
    "typescript": "~4.5.4",
    "vite": "~2.7.4",
    "vite-plugin-compression": "~0.3.6",
    "vite-plugin-env-compatible": "~1.1.1",
    "vite-plugin-eslint": "~1.3.0",
    "vite-plugin-pages": "~0.19.6",
    "vite-plugin-stylelint": "~1.1.0",
    "vite-tsconfig-paths": "~3.3.17"
  }
}
